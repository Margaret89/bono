extends ../layout/main

block variables
	- var title = 'Leads';

block content
	.player
		.player__move
			+svg('ic-many-dots','ic-many-dots', '6', '10')

		.player-user
			.player-user__img(style="background-image: url(assets/img/abraham.png)")
			.player-user__content
				.player-user__name Abraham Luis
				.player-user__status
					+svg('ic-phone','ic-phone', '10', '10')
					|Incoming call

		.player__actions
			.player__prev
				+svg('ic-15-prev','ic-15-prev', '19', '22')

			.player__play
				+svg('ic-play','ic-play', '11', '14')

			.player__next
				+svg('ic-15-next','ic-15-next', '19', '22')

		.player-timeline
			.player-timeline__text 01:40

			.player-timeline__content
				.player-timeline__line(style="width:37%")
				.player-timeline__dot(style="left:5%")
				.player-timeline__dot(style="left:41%")
				.player-timeline__dot(style="left:46%")
				.player-timeline__dot(style="left:89%")

			.player-timeline__text 13:25

		.player__review
			+svg('ic-review','ic-review', '19', '19')

		.player-volume
			.player-volume__icon
				+svg('ic-volume','ic-volume', '24', '17')

			.player-volume__line
				.player-volume__progress(style="width:100%")
				.player-volume__handle(style="left:100%")

		.btn.btn_white End Call

